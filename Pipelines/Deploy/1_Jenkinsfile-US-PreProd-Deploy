/* groovylint-disable CompileStatic */
// Uses Declarative syntax to run commands inside a container.
pipeline {
    agent {
        kubernetes {
            yaml '''
        apiVersion: v1
        kind: Pod
        environment:
            name: "NODE_OPTIONS"
            value: "--max-old-space-size=4096"
        spec:
          containers:
          - name: jnlp  
            resources:
              requests:
                memory: "2048Mi"
                cpu: "500m"
              limits:
                memory: "4096Mi"
                cpu: "1000m"
                
          - name: nodejs
            image: node:18-alpine
            command:
            - cat
            tty: true

          - name: gcloud
            image: google/cloud-sdk:latest
            command:
            - cat
            tty: true
        '''        
        }
    }

    environment {
        APP_ENGINE_PROJECT_ID = 'sqpreprod-us'
        GCS_BUCKET_NAME = 'sqinternational-cicd.appspot.com'
        ENVIRONMENT = 'MASTER'
        APP_ENGINE_VERSION = 'v2' // This should be updated with the build number
        YAML = 'app.yaml'
        YAML1 = 'time2eat.yaml'
        YAML2 = 'baweb.yaml'
        //GOOGLE_APPLICATION_CREDENTIALS = credentials('gcloud-creds')
        GLOBAL_COUNT = '0'
    }
   
    stages {  
        stage('Deploy NodeJS Apps') {
            steps {
                parallel (
                    controldesk: {
                            container('gcloud') {
                                script {
                                    sh "gcloud config set project $APP_ENGINE_PROJECT_ID"
                                     def APP_NAME = "controldesk"
                                    sh "mkdir -p ${APP_NAME}-latest"                                
                                    echo "current workspace: ${WORKSPACE}"                                    
                                    STORAGE_OBJECT_SOURCE_PATH = sh (
                                        script: "gcloud storage ls gs://$GCS_BUCKET_NAME/$ENVIRONMENT/${APP_NAME} | sort --ignore-case -rV | head -n 1",
                                        returnStdout: true
                                    ).trim()
                                    echo "Storage Object Source Path: ${STORAGE_OBJECT_SOURCE_PATH}"
                                    sh "gcloud storage cp ${STORAGE_OBJECT_SOURCE_PATH}* ${WORKSPACE}/${APP_NAME}-latest/ --recursive"
                                   
                                    dir("${APP_NAME}-latest") {
                                        sh "gcloud app deploy --version=$APP_ENGINE_VERSION --quiet $YAML --no-promote"
                                    }

                                    def service_url = sh (
                                        script: "gcloud app browse --service=$APP_NAME --version=$APP_ENGINE_VERSION --no-launch-browser --format='value(url)'",
                                        returnStdout: true
                                    ).trim()
                                    echo "Service URL: ${service_url}"

                                    def CHECK_URL_STATUS = "curl --write-out %{http_code} --silent --output /dev/null '${service_url}/control-desk/login'"
                                    sh "${CHECK_URL_STATUS} > commandResult"
                                    env.status = readFile('commandResult').trim()
                                    sh "echo ${env.status}"  

                                    if (env.status == "200") {
                                        GLOBAL_COUNT = (GLOBAL_COUNT.toInteger() + 1).toString()
                                        echo "Global count : ${GLOBAL_COUNT} of 12"                                   
                                        //sh "gcloud app services set-traffic $APP_NAME --splits $APP_ENGINE_VERSION=1 --quiet"
                                    }
                                    else {  
                                        echo "Failed with Status Code : ${env.status}"
                                    }
                                }
                            }
                        },
                    time2eat: {
                        container('gcloud') {                           
                            script {
                                    sh "gcloud config set project $APP_ENGINE_PROJECT_ID"

                                    def APP_NAME = "time2eat"
                                    sh "mkdir -p ${APP_NAME}-latest"                                
                                    echo "current workspace: ${WORKSPACE}/${APP_NAME}"
                                   
                                    STORAGE_OBJECT_SOURCE_PATH = sh (
                                        script: "gcloud storage ls gs://$GCS_BUCKET_NAME/$ENVIRONMENT/${APP_NAME} | sort --ignore-case -rV | head -n 1",
                                        returnStdout: true
                                    ).trim()
                                    echo "Storage Object Source Path: ${STORAGE_OBJECT_SOURCE_PATH}"
                                    sh "gcloud storage cp ${STORAGE_OBJECT_SOURCE_PATH}* ${WORKSPACE}/${APP_NAME}-latest --recursive"
                                   
                                    dir("${APP_NAME}-latest") {
                                        sh "gcloud app deploy --version=$APP_ENGINE_VERSION --quiet $YAML1 --no-promote"
                                        sh "gcloud app deploy --version=$APP_ENGINE_VERSION --quiet $YAML2 --no-promote"
                                    }
                                    def service_url2 = sh (
                                        script: "gcloud app browse --service='time2eat' --version=$APP_ENGINE_VERSION --no-launch-browser --format='value(url)'",
                                        returnStdout: true
                                    ).trim()
                                    echo "Service URL: ${service_url2}"

                                    def CHECK_URL_STATUS = "curl --write-out %{http_code} --silent --output /dev/null '${service_url2}/time2eat/main'"
                                    sh "${CHECK_URL_STATUS} > commandResult"
                                    env.status = readFile('commandResult').trim()
                                    sh "echo ${env.status}"  

                                    if (env.status == "200") {
                                        GLOBAL_COUNT = (GLOBAL_COUNT.toInteger() + 1).toString()
                                        echo "Global count : ${GLOBAL_COUNT} of 12"                                   
                                        //sh "gcloud app services set-traffic $APP_NAME --splits $APP_ENGINE_VERSION=1 --quiet"
                                    }
                                    else {  
                                        echo "Failed with Status Code : ${env.status}"
                                    }

                                    def service_url3 = sh (
                                        script: "gcloud app browse --service='baweb' --version=$APP_ENGINE_VERSION --no-launch-browser --format='value(url)'",
                                        returnStdout: true
                                    ).trim()
                                    echo "Service URL: ${service_url3}"

                                    def CHECK_URL_STATUS2 = "curl --write-out %{http_code} --silent --output /dev/null '${service_url3}/baweb/main'"
                                    sh "${CHECK_URL_STATUS2} > commandResult"
                                    env.status = readFile('commandResult').trim()
                                    sh "echo ${env.status}"  

                                    if (env.status == "200") {
                                        GLOBAL_COUNT = (GLOBAL_COUNT.toInteger() + 1).toString()
                                        echo "Global count : ${GLOBAL_COUNT} of 12"                                    
                                        //sh "gcloud app services set-traffic 'baweb' --splits $APP_ENGINE_VERSION=1 --quiet"
                                    }
                                    else {  
                                        echo "Failed with Status Code : ${env.status}"
                                    }
                            }
                        }
                    },
                    webORT: {
                        container('gcloud') {                            
                            script {
                                sh "gcloud config set project $APP_ENGINE_PROJECT_ID"
                                def APP_NAME = "web-ort"
                                sh "mkdir -p ${APP_NAME}-latest"                                
                                echo "current workspace: ${WORKSPACE}/${APP_NAME}"
                               
                                STORAGE_OBJECT_SOURCE_PATH = sh (
                                    script: "gcloud storage ls gs://$GCS_BUCKET_NAME/$ENVIRONMENT/${APP_NAME} | sort --ignore-case -rV | head -n 1",
                                    returnStdout: true
                                ).trim()
                                echo "Storage Object Source Path: ${STORAGE_OBJECT_SOURCE_PATH}"
                                sh "gcloud storage cp ${STORAGE_OBJECT_SOURCE_PATH}* ${WORKSPACE}/${APP_NAME}-latest --recursive"
                                   
                                dir("${APP_NAME}-latest") {
                                    sh "gcloud app deploy --version=$APP_ENGINE_VERSION --quiet $YAML --no-promote"
                                }

                                def service_url4 = sh (
                                        script: "gcloud app browse --service=$APP_NAME --version=$APP_ENGINE_VERSION --no-launch-browser --format='value(url)'",
                                        returnStdout: true
                                    ).trim()

                                echo "Service URL: ${service_url4}"

                                def CHECK_URL_STATUS = "curl --write-out %{http_code} --silent --output /dev/null '${service_url4}/$APP_NAME/main'"
                                sh "${CHECK_URL_STATUS} > commandResult"
                                env.status = readFile('commandResult').trim()
                                sh "echo ${env.status}"  

                                if (env.status == "200") {
                                    GLOBAL_COUNT = (GLOBAL_COUNT.toInteger() + 1).toString()
                                    echo "Global count : ${GLOBAL_COUNT} of 12"                                   
                                    //sh "gcloud app services set-traffic $APP_NAME --splits $APP_ENGINE_VERSION=1 --quiet"
                                }
                                else {  
                                    echo "Failed with Status Code : ${env.status}"
                                }                                
                            }
                        }
                    }
                ) 
            }
        }    
        stage('Clone smartq-backend') { 
            steps { 
                sh "mkdir -p smartq-cloud-backend"
                dir('smartq-cloud-backend') {                        
                    git branch: 'preprodindia',
                    credentialsId: '205fee1d-5909-4587-abe4-8c50bdc37556',
                    url: 'https://vrp63531@bitbucket.org/bottlelabtech/smartq-cloud-backend.git'

                dir ('py3_project/py3_backend/bqreports/admindashboard/'){
                        sh 'rm -rf ./{*,.*}'
                    }
                dir ('py3_project/py3_backend/bqreports/reports/'){
                        sh 'rm -rf ./{*,.*}'
                    }
                dir ('eWallet'){
                        sh 'rm -rf ./{*,.*}'
                    }     
                sh 'ls -lrt'
                }
            } 
        }
        stage('copy build output - ewallet, admin & vendor dash-board') {
            steps {                
                dir ('smartq-cloud-backend') {
                    container('gcloud') {
                        sh "echo ${WORKSPACE}"
                        sh "gcloud config set project $APP_ENGINE_PROJECT_ID"

                        //sh 'gcloud storage cp gs://sqinternational-cicd.appspot.com/$ENVIRONMENT/generic-admin-dashboard/generic-admin-dashboard-Latest/* ${WORKSPACE}/smartq-cloud-backend/py3_project/py3_backend/bqreports/admindashboard --recursive'
                                                
                        script {  
                                def APP_NAME = "generic-admin-dashboard" 
                                STORAGE_OBJECT_SOURCE_PATH = sh (
                                    script: "gcloud storage ls gs://$GCS_BUCKET_NAME/$ENVIRONMENT/${APP_NAME} | sort --ignore-case -rV | head -n 1",
                                    returnStdout: true
                                ).trim()
                                echo "Storage Object Source Path: ${STORAGE_OBJECT_SOURCE_PATH}"
                                sh "gcloud storage cp ${STORAGE_OBJECT_SOURCE_PATH}* ${WORKSPACE}/smartq-cloud-backend/py3_project/py3_backend/bqreports/admindashboard --recursive"
                        }
                        
                        //sh 'gcloud storage cp gs://sqinternational-cicd.appspot.com/$ENVIRONMENT/vendor-dashboard/vendor-dashboard-Latest/* ${WORKSPACE}/smartq-cloud-backend/py3_project/py3_backend/bqreports/reports --recursive'

                        script {  
                                def APP_NAME = "vendor-dashboard" 
                                STORAGE_OBJECT_SOURCE_PATH = sh (
                                    script: "gcloud storage ls gs://$GCS_BUCKET_NAME/$ENVIRONMENT/${APP_NAME} | sort --ignore-case -rV | head -n 1",
                                    returnStdout: true
                                ).trim()
                                echo "Storage Object Source Path: ${STORAGE_OBJECT_SOURCE_PATH}"
                                sh "gcloud storage cp ${STORAGE_OBJECT_SOURCE_PATH}* ${WORKSPACE}/smartq-cloud-backend/py3_project/py3_backend/bqreports/reports --recursive"
                        }

                        //sh 'gcloud storage cp gs://sqinternational-cicd.appspot.com/$ENVIRONMENT/ewallet/ewallet-Latest/* ${WORKSPACE}/smartq-cloud-backend/eWallet --recursive'
                        script {  
                                def APP_NAME = "ewallet" 
                                STORAGE_OBJECT_SOURCE_PATH = sh (
                                    script: "gcloud storage ls gs://$GCS_BUCKET_NAME/$ENVIRONMENT/${APP_NAME} | sort --ignore-case -rV | head -n 1",
                                    returnStdout: true
                                ).trim()
                                echo "Storage Object Source Path: ${STORAGE_OBJECT_SOURCE_PATH}"
                                sh "gcloud storage cp ${STORAGE_OBJECT_SOURCE_PATH}* ${WORKSPACE}/smartq-cloud-backend/eWallet --recursive"
                        }        

                        dir ('py3_project/py3_backend/bqreports/admindashboard') {
                            sh 'ls -lrt'
                        }
                        dir ('py3_project/py3_backend/bqreports/reports') {
                            sh 'ls -lrt'
                        }
                        dir ('eWallet') {
                            sh 'ls -lrt'
                        }
                    }
                }
            }
        }
        stage('Deploy Smartq Backend') {
            steps {
                parallel (
                    appservice: {
                            container('gcloud') {
                                sh "gcloud config set project $APP_ENGINE_PROJECT_ID"
                                dir ('smartq-cloud-backend'){
                                    sh "ls *.yaml"
                                    sh "gcloud app deploy --version=$APP_ENGINE_VERSION --quiet time2eat_appservice.yaml --no-promote"
                                }
                                script {
                                    def service_url5 = sh (
                                        script: "gcloud app browse --service='appservice' --version=$APP_ENGINE_VERSION --no-launch-browser --format='value(url)'",
                                        returnStdout: true
                                    ).trim()
                                    echo "Service URL: ${service_url5}"

                                    def CHECK_URL_STATUS = "curl --write-out %{http_code} --silent --output /dev/null '${service_url5}'"
                                    sh "${CHECK_URL_STATUS} > commandResult"
                                    env.status = readFile('commandResult').trim()
                                    sh "echo ${env.status}"  

                                    if (env.status == "200") {
                                        GLOBAL_COUNT = (GLOBAL_COUNT.toInteger() + 1).toString()
                                        echo "Global count : ${GLOBAL_COUNT} of 12"                                   
                                        sh "gcloud app services set-traffic 'appservice' --splits $APP_ENGINE_VERSION=1 --quiet"
                                    }
                                    else {  
                                        echo "Failed with Status Code : ${env.status}"
                                    }
                                }                                
                            }
                        },
                    backgroundtasks: {
                            container('gcloud') {
                                sh "gcloud config set project $APP_ENGINE_PROJECT_ID"
                                dir ('smartq-cloud-backend'){
                                    sh "gcloud app deploy --version=$APP_ENGINE_VERSION --quiet background.yaml --no-promote"
                                }
                                script {
                                def service_url6 = sh (
                                        script: "gcloud app browse --service='backgroundtasks' --version=$APP_ENGINE_VERSION --no-launch-browser --format='value(url)'",
                                        returnStdout: true
                                    ).trim()
                                    echo "Service URL: ${service_url6}"

                                    def CHECK_URL_STATUS = "curl --write-out %{http_code} --silent --output /dev/null '${service_url6}'"
                                    sh "${CHECK_URL_STATUS} > commandResult"
                                    env.status = readFile('commandResult').trim()
                                    sh "echo ${env.status}"  

                                    if (env.status == "200") {
                                        GLOBAL_COUNT = (GLOBAL_COUNT.toInteger() + 1).toString()
                                        echo "Global count : ${GLOBAL_COUNT} of 12"                                   
                                        sh "gcloud app services set-traffic 'backgroundtasks' --splits $APP_ENGINE_VERSION=1 --quiet"
                                    }
                                    else {  
                                        echo "Failed with Status Code : ${env.status}"
                                    }
                                }               
                            }
                        },
                    taskqservice: {
                            container('gcloud') {
                                sh "gcloud config set project $APP_ENGINE_PROJECT_ID"
                                dir ('smartq-cloud-backend'){
                                    sh "gcloud app deploy --version=$APP_ENGINE_VERSION --quiet taskqservice.yaml --no-promote"
                                }
                                script {                            
                                    def service_url7 = sh (
                                        script: "gcloud app browse --service='taskqservice' --version=$APP_ENGINE_VERSION --no-launch-browser --format='value(url)'",
                                        returnStdout: true
                                    ).trim()
                                    echo "Service URL: ${service_url7}"

                                    def CHECK_URL_STATUS = "curl --write-out %{http_code} --silent --output /dev/null '${service_url7}'"
                                    sh "${CHECK_URL_STATUS} > commandResult"
                                    env.status = readFile('commandResult').trim()
                                    sh "echo ${env.status}"  

                                    if (env.status == "200") {
                                        GLOBAL_COUNT = (GLOBAL_COUNT.toInteger() + 1).toString()
                                        echo "Global count : ${GLOBAL_COUNT} of 12"                                   
                                        sh "gcloud app services set-traffic 'taskqservice' --splits $APP_ENGINE_VERSION=1 --quiet"
                                    }
                                    else {  
                                        echo "Failed with Status Code : ${env.status}"
                                    }
                                }      
                            }
                        }//,
                    // eWallet: {
                    //         container('gcloud') {
                    //             sh "gcloud config set project $APP_ENGINE_PROJECT_ID"
                    //             dir ('smartq-cloud-backend'){
