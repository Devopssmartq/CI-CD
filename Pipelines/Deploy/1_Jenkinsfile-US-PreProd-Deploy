                    //             }
                    //             script {

                    //                 def service_url8 = sh (
                    //                     script: "gcloud app browse --service='eWallet' --version=$APP_ENGINE_VERSION --no-launch-browser --format='value(url)'",
                    //                     returnStdout: true
                    //                 ).trim()
                    //                 echo "Service URL: ${service_url8}"

                    //                 def CHECK_URL_STATUS = "curl --write-out %{http_code} --silent --output /dev/null '${service_url8}/eWallet/transactions'"
                    //                 sh "${CHECK_URL_STATUS} > commandResult"
                    //                 env.status = readFile('commandResult').trim()
                    //                 sh "echo ${env.status}"  

                    //                 if (env.status == "200") {
                    //                     GLOBAL_COUNT = (GLOBAL_COUNT.toInteger() + 1).toString()
                    //                     echo "Global count : ${GLOBAL_COUNT} of 12"                                   
                    //                     sh "gcloud app services set-traffic 'eWallet' --splits $APP_ENGINE_VERSION=1 --quiet"
                    //                 }
                    //                 else {  
                    //                     echo "Failed with Status Code : ${env.status}"
                    //                 }
                    //             }
                    //         }
                    //     }                    
                )
            }
        }
        stage('Deploy to BQ Reports') {
            steps {
                container('gcloud') {
                    sh "gcloud config set project $APP_ENGINE_PROJECT_ID"
                    dir ('smartq-cloud-backend') {
                        dir ('py3_project') {
                            sh "ls *.yaml"
                            sh "gcloud app deploy --version=$APP_ENGINE_VERSION --quiet bqreports.yaml --no-promote"
                        }
                    }
                    script {

                        def service_url9 = sh (
                            script: "gcloud app browse --service='bqreports' --version=$APP_ENGINE_VERSION --no-launch-browser --format='value(url)'",
                            returnStdout: true
                        ).trim()
                        echo "Service URL: ${service_url9}"

                        def CHECK_URL_STATUS = "curl --write-out %{http_code} --silent --output /dev/null '${service_url9}'"
                        sh "${CHECK_URL_STATUS} > commandResult"
                        env.status = readFile('commandResult').trim()
                        sh "echo ${env.status}"  

                        if (env.status == "200") {
                            GLOBAL_COUNT = (GLOBAL_COUNT.toInteger() + 1).toString()
                            echo "Global count : ${GLOBAL_COUNT} of 12"                                   
                            sh "gcloud app services set-traffic 'bqreports' --splits $APP_ENGINE_VERSION=1 --quiet"
                        }
                        else {  
                            echo "Failed with Status Code : ${env.status}"
                        }
                    }
                }               
            }
        }
        stage('Clone sq_microservices_backend') { 
            steps { 
                sh "mkdir -p sq_microservices_backend"
                dir('sq_microservices_backend') {                        
                    git branch: 'preprodsetup',
                    credentialsId: '205fee1d-5909-4587-abe4-8c50bdc37556',
                    url: 'https://vrp63531@bitbucket.org/bottlelabtech/sq_microservices_backend.git'
                                   
                sh 'ls -lrt'
                }
            } 
        }
        stage('Deploy Microservices') {
            steps {
                parallel (
                    appms: {
                            container('gcloud') {
                                sh "echo ${WORKSPACE}"
                                sh "gcloud config set project $APP_ENGINE_PROJECT_ID"
                                dir ('sq_microservices_backend') {
                                    sh 'cp -r commonlib appms'
                                    dir ('appms') {
                                        sh "ls *.yaml"
                                        sh "gcloud app deploy --version=$APP_ENGINE_VERSION --quiet sqpreprod-us.yaml --no-promote"
                                    }
                                }
                                script {

                                    def service_url10 = sh (
                                        script: "gcloud app browse --service='default' --version=$APP_ENGINE_VERSION --no-launch-browser --format='value(url)'",
                                        returnStdout: true
                                    ).trim()
                                    echo "Service URL: ${service_url10}"

                                    def CHECK_URL_STATUS = "curl --write-out %{http_code} --silent --output /dev/null '${service_url10}'"
                                    sh "${CHECK_URL_STATUS} > commandResult"
                                    env.status = readFile('commandResult').trim()
                                    sh "echo ${env.status}"  

                                    if (env.status == "200") {
                                        GLOBAL_COUNT = (GLOBAL_COUNT.toInteger() + 1).toString()
                                        echo "Global count : ${GLOBAL_COUNT} of 12"                                   
                                        //sh "gcloud app services set-traffic 'default' --splits $APP_ENGINE_VERSION=1 --quiet"
                                    }
                                    else {  
                                        echo "Failed with Status Code : ${env.status}"
                                    }
                                }
                            } 
                        },
                    dashboardms: {
                            container('gcloud') {
                                sh "echo ${WORKSPACE}"
                                sh "gcloud config set project $APP_ENGINE_PROJECT_ID"

                                dir ('sq_microservices_backend') {
                                    sh 'cp -r commonlib dashboardms'
                                    dir ('dashboardms'){
                                        sh "ls *.yaml"
                                        sh "gcloud app deploy --version=$APP_ENGINE_VERSION --quiet sqpreprod-us.yaml --no-promote"
                                    }                                  
                                }
                                script {

                                    def service_url11 = sh (
                                        script: "gcloud app browse --service='dashboard' --version=$APP_ENGINE_VERSION --no-launch-browser --format='value(url)'",
                                        returnStdout: true
                                    ).trim()
                                    echo "Service URL: ${service_url11}"

                                    def CHECK_URL_STATUS = "curl --write-out %{http_code} --silent --output /dev/null '${service_url11}'"
                                    sh "${CHECK_URL_STATUS} > commandResult"
                                    env.status = readFile('commandResult').trim()
                                    sh "echo ${env.status}"  

                                    if (env.status == "200") {
                                        GLOBAL_COUNT = (GLOBAL_COUNT.toInteger() + 1).toString()
                                        echo "Global count : ${GLOBAL_COUNT} of 12"                                   
                                        //sh "gcloud app services set-traffic 'dashboard' --splits $APP_ENGINE_VERSION=1 --quiet"
                                    }
                                    else {  
                                        echo "Failed with Status Code : ${env.status}"
                                    }
                                }
                            }
                        }//,
                    // sso_flex: {
