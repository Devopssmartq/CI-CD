                    //             }
                    //            }
                    //         }
                    //     }              
                )
            }
        }
        stage('Promote') {
           steps {
                container('gcloud') {
                    sh "gcloud config set project $APP_ENGINE_PROJECT_ID"
                    script { 
                        env.count = GLOBAL_COUNT.toInteger().toString()
                        echo "Global count : ${env.count}"                  
                        if (env.count == "9") {
                            echo "Start Promoting.."     
                            //sh "gcloud app services set-traffic 'controldesk' --splits $APP_ENGINE_VERSION=1 --quiet"                             
                            //sh "gcloud app services set-traffic 'bqreports' --splits $APP_ENGINE_VERSION=1 --quiet"
                            //sh "gcloud app services set-traffic 'web-ort' --splits $APP_ENGINE_VERSION=1 --quiet"
                        }
                        else {  
                            echo "Promote halted.."
                        }
                   }
                }               
            }  
        }      
    }
}