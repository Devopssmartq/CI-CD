                    //                 script: "gcloud app browse --service='sso' --version=$APP_ENGINE_VERSION --no-launch-browser --format='value(url)'",
                    //                 returnStdout: true
                    //             ).trim()
                    //             echo "Service URL: ${service_url12}"

                    //             def CHECK_URL_STATUS = "curl --write-out %{http_code} --silent --output /dev/null '${service_url12}'"
                    //             sh "${CHECK_URL_STATUS} > commandResult"
                    //             env.status = readFile('commandResult').trim()
                    //             sh "echo ${env.status}"  

                    //             if (env.status == "200") {
                    //                 GLOBAL_COUNT = (GLOBAL_COUNT.toInteger() + 1).toString()
                    //                 echo "Global count : ${GLOBAL_COUNT} of 12"                                   
