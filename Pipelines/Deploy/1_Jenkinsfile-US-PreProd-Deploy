/* groovylint-disable CompileStatic */
// Uses Declarative syntax to run commands inside a container.
def getURL_Status(Map args) {
    echo "service name is ${args.service_name}, version name is ${args.version_name}, app name is ${args.url_suffix}"

    def service_url = sh (
                script: "gcloud app browse --service=${args.service_name} --version=${args.version_name} --no-launch-browser --format='value(url)'",
                returnStdout: true
    ).trim()

    echo "Service URL: ${service_url}"

    def CHECK_URL_STATUS = "curl --retry 2 -f --retry-all-errors --retry-delay 5 --write-out %{http_code} --silent --output /dev/null '${service_url}/${args.url_suffix}'"
    sh "${CHECK_URL_STATUS} > commandResult"
    env.status = readFile('commandResult').trim()
    sh "echo ${env.status}"  

    if (env.status == "200") {
        GLOBAL_COUNT = (GLOBAL_COUNT.toInteger() + 1).toString()
        echo "Global count : ${GLOBAL_COUNT} of 11"                                   
    }
    else {  
            echo "Failed with Status Code : ${env.status}"
    }
}
